1.3
log
@Autolaunch
@
text
@d32 5
a36 7
	@@for a in $(ACCS); do \
		d=/acc/dsc/$$a/$(CPU)/tsvy;\
		echo Installing tsvytest in $$d;\
		dsc_install tsvytest.$(CPU) $$d;\
		echo Installing configuration file; \
		dsc_install tsvytest.config $$d;\
	done;
@


