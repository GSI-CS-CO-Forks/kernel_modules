####### Implicit rules

.SUFFIXES: .c .o .cpp .cc .cxx .C

.cpp.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cc.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.cxx.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.C.o:
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o $@ $<

.c.o:
	$(CC) -c $(CFLAGS) $(INCPATH) -o $@ $<

####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(OBJCOMP) $(LIBS)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: cutecom.pro  /usr/lib/qt-3.3/mkspecs/default/qmake.conf /usr/lib/qt-3.3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile cutecom.pro
qmake: 
	@$(QMAKE) -o Makefile cutecom.pro

dist: 
	@mkdir -p .obj/cutecom && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/cutecom/ && ( cd `dirname .obj/cutecom` && $(TAR) cutecom.tar cutecom && $(GZIP) cutecom.tar ) && $(MOVE) `dirname .obj/cutecom`/cutecom.tar.gz . && $(DEL_FILE) -r .obj/cutecom

mocclean:
	-$(DEL_FILE) $(OBJMOC)
	-$(DEL_FILE) $(SRCMOC)

uiclean:
	-$(DEL_FILE) $(UICIMPLS) $(UICDECLS)

yaccclean:
lexclean:
clean: mocclean uiclean
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) $(TARGET)


FORCE:

####### Compile

.obj/main.o: main.cpp qcppdialogimpl.h \
		cutecommdlg.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/main.o main.cpp

.obj/qcppdialogimpl.o: qcppdialogimpl.cpp qcppdialogimpl.h \
		cutecommdlg.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/qcppdialogimpl.o qcppdialogimpl.cpp

cutecommdlg.h: cutecommdlg.ui 
	$(UIC) cutecommdlg.ui -o cutecommdlg.h

cutecommdlg.cpp: cutecommdlg.h cutecommdlg.ui 
	$(UIC) cutecommdlg.ui -i cutecommdlg.h -o cutecommdlg.cpp

.obj/cutecommdlg.o: ./cutecommdlg.cpp ./cutecommdlg.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/cutecommdlg.o ./cutecommdlg.cpp

.obj/moc_qcppdialogimpl.o: .moc/moc_qcppdialogimpl.cpp  qcppdialogimpl.h cutecommdlg.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_qcppdialogimpl.o .moc/moc_qcppdialogimpl.cpp

.obj/moc_cutecommdlg.o: .moc/moc_cutecommdlg.cpp  ./cutecommdlg.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_cutecommdlg.o .moc/moc_cutecommdlg.cpp

.moc/moc_qcppdialogimpl.cpp: $(MOC) qcppdialogimpl.h
	$(MOC) qcppdialogimpl.h -o .moc/moc_qcppdialogimpl.cpp

.moc/moc_cutecommdlg.cpp: $(MOC) ./cutecommdlg.h
	$(MOC) ./cutecommdlg.h -o .moc/moc_cutecommdlg.cpp

####### Install

install_target: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/usr/local/bin/" || $(MKDIR) "$(INSTALL_ROOT)/usr/local/bin/"
	-$(INSTALL_FILE) "$(QMAKE_TARGET)" "$(INSTALL_ROOT)/usr/local/bin/$(QMAKE_TARGET)"

uninstall_target: 
	-$(DEL_FILE) "$(INSTALL_ROOT)/usr/local/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) "$(INSTALL_ROOT)/usr/local/bin/"


install_desktop: all 
	@$(CHK_DIR_EXISTS) "$(INSTALL_ROOT)/usr/share/applnk/Utilities/" || $(MKDIR) "$(INSTALL_ROOT)/usr/share/applnk/Utilities/"
	-$(INSTALL_FILE) "cutecom.desktop" "$(INSTALL_ROOT)/usr/share/applnk/Utilities/"


uninstall_desktop: 
	-$(DEL_FILE) -r "$(INSTALL_ROOT)/usr/share/applnk/Utilities/cutecom.desktop"
	-$(DEL_DIR) "$(INSTALL_ROOT)/usr/share/applnk/Utilities/"


install: install_target install_desktop  

uninstall: uninstall_target uninstall_desktop  

